{
    "servers": {
        "kozen": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "ts-node",
                "bin/mcp.ts"
            ],
            "env": {
                "MDB_URI": "<MDB_URI>",
                "MDB_MASTER_KEY": "<MDB_MASTER_KEY>",
