{
    "name": "AtlasBasic",
    "description": "Basic template for MongoDB Atlas component",
    "version": "1.0.0",
    "engine": "1.0.0",
    "release": "20241201",
    "requires": [],
    "deploymentMode": "synchronous",
    "stack": {
        "orchestrator": "Pulumi",
        "input": [
            {
                "type": "environment",
                "name": "PULUMI_CONFIG_PASSPHRASE"
            },
            {
                "type": "environment",
                "name": "PULUMI_BACKEND_URL"
            }
        ],
        "setup": [
            {
                "type": "environment",
                "name": "aws:region",
                "value": "AWS_REGION",
                "default": "us-east-1"
            },
            {
                "type": "protected",
                "name": "aws:accessKey",
                "value": "AWS_ACCESS_KEY_ID",
                "default": ""
            },
            {
                "type": "protected",
                "name": "aws:secretKey",
                "value": "AWS_SECRET_ACCESS_KEY",
                "default": ""
            }
        ],
        "components": []
    }
}